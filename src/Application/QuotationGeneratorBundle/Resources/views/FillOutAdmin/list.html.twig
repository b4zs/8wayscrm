{% extends '@SonataAdmin/CRUD/base_list.html.twig' %}

{% block list_filters %}
<div class="box box-primary">
	<div class="box-header">
		<h4 class="box-title filter_legend">Next Questions</h4>
	</div>

	<div class="box-body">
		<ul>
		{% for createUrl, questionText in admin.buildQuestionStackList(admin.parent.subject) %}
		<li>
			<a href="{{ createUrl }}">{{ questionText }}</a>
		</li>
		{% endfor %}
		</ul>
	</div>
</div>
{% endblock %}

